<multistatus xmlns="DAV:">
    <response>
        <href>dcbox:/</href>
        <propstat>
            <prop>
                <resourcetype>
                    <collection/>
                </resourcetype>
                <acl xml:base="https://demo.personium.io/app-myboard/__role/__/" xmlns:dc="urn:x-dcl:xmlns">
                    <ace>
                        <principal>
                            <href>MyBoardViewer</href>
                        </principal>
                        <grant>
                            <privilege>
                                <read/>
                            </privilege>
                        </grant>
                    </ace>
                </acl>
            </prop>
        </propstat>
    </response>
    <response>
        <href>dcbox:/MyBoardBox</href>
        <propstat>
            <prop>
                <resourcetype>
                    <collection/>
                </resourcetype>
                <acl xml:base="https://demo.personium.io/app-myboard/__role/__/" xmlns:dc="urn:x-dcl:xmlns">
                    <ace>
                        <principal>
                            <href>MyBoardOperator</href>
                        </principal>
                        <grant>
                            <privilege>
                                <write/>
                            </privilege>
                            <privilege>
                                <read/>
                            </privilege>
                        </grant>
                    </ace>
                    <ace>
                        <principal>
                            <href>MyBoardViewer</href>
                        </principal>
                        <grant>
                            <privilege>
                                <read/>
                            </privilege>
                        </grant>
                    </ace>
                </acl>
                <dc:my-board.json xmlns:dc="urn:x-dc1:xmlns">my-board.json</dc:my-board.json>
            </prop>
        </propstat>
    </response>
    <response>
        <href>dcbox:/MyBoardBox/my-board.json</href>
        <propstat>
            <prop>
                <getcontenttype>text/plan</getcontenttype>
            </prop>
        </propstat>
    </response>
</multistatus>
